// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id String @id @default(cuid())
  title String
  description String
  start_at DateTime
  due_at DateTime
  end_at DateTime
  product_owner_id Int
  advancement Int
}
